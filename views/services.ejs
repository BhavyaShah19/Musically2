    <!-- Modal -->
    <div id="exampleModalToggle" class="modal fade" role="dialog" aria-hidden="true"
        aria-labelledby="exampleModalToggleLabel" tabindex="-1">
        >
        <div class="modal-dialog modal-dialog-centered modal-dialog-scrollable modal-lg">

            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalToggleLabel">Billing Details</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="container">
                        <form action="/checkout" method="POST">
                            <div class="form-group">
                                <label for="fname"><i class="fa fa-user"></i> Full Name:</label>
                                <input id="fname" type="text" name="firstname" class="form-control"
                                    placeholder="John M. Doe" />
                            </div>
                            <div class="form-group">
                                <label for="email"><i class="fa fa-envelope"></i> Email:</label>
                                <input id="email" type="text" name="email" class="form-control"
                                    placeholder="john@example.com" />
                            </div>
                            <div class="form-group">
                                <label for="phone"><i class="fa fa-phone"></i> Whataspp Number:</label>
                                <input id="phone" type="text" name="phnumber" class="form-control"
                                    placeholder="9521659235" />
                            </div>
                            <div class="form-group">
                                <label for="adr"><i class="fa fa-address-card-o"></i> Address:</label>
                                <input id="adr" type="text" name="address" class="form-control"
                                    placeholder="542 W. 15th Street" />
                            </div>
                            <div class="checkbox">
                                <label><input type="checkbox"> Remember me</label>
                            </div>
                            <div class="form-group">
                                <label for="instru"><i class="fa fa-music"></i>Instrument</label>
                                <p id="instru"> </p>
                                <input id="instruInput" type="text" name="instru" />
                            </div>
                            <div class="form-group">
                                <label for="plan"><i class="fa fa-music"></i>You have chosen plan:</label>
                                <p id="plan"></p>
                            </div>
                            <div class="form-group">
                                <p><i class="fa fa-chalkboard-teacher"></i>Want a Teacher?</p>
                                <div class="form-check">
                                    <input class="form-check-input" type="radio" name="iwant" value="1" checked
                                        id="yes">
                                    <label class="form-check-label" for="yes">
                                        Yes
                                    </label>
                                </div>
                                <div class="form-check">
                                    <input class="form-check-input" type="radio" name="iwant" value="0" id="no">
                                    <label class="form-check-label" for="no">
                                        No
                                    </label>
                                </div>
                            </div>
                            <div class="form-group">
                                <h3 class="totalh3">Your Total amount:</h3>
                                <p class="totalamt" id="totAmt"></p>
                                <input class="totalamt" id="totAmtBox" type="text" name="amt" readonly="readonly" />
                            </div>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                                <button type="submit" class="btn btn-primary" id="btncheck">Proceed to checkout</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
